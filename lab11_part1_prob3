#include <iostream>
using namespace std;
bool ismirrored(const int *a,const int *b,int  size);
int main() {
    int size;
    cin>>size;
    int arr[size],arr2[size];
    for(int i=0;i<size;i++) {
        cin>>arr[i];
    }
    for(int i=0;i<size;i++) {
        cin>>arr2[i];
    }
    cout<<ismirrored(arr,arr2,size);
}
bool ismirrored(const int *a,const int *b,int  size) {
    const int *start=a;
    const int *end=b+size-1;
    for (int i=0;i<size;i++) {
        if (*start!=*end) {
            return false;
        }
        start++;
        end--;
    }
    return true;
}
